{
  "name": "@shortwave/ts-proto",
  "version": "1.84.3",
  "main": "build/plugin.js",
  "repository": "github:shortwave/ts-proto",
  "bin": {
    "protoc-gen-ts_proto": "./protoc-gen-ts_proto"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn tsc",
    "prepublish": "npm run build",
    "setup": "cd ./integration && ./pbjs.sh && ./update-bins.sh && ./codegen.sh",
    "test": "yarn jest -c jest.config.js --maxWorkers=2",
    "release": "np",
    "prettier": "prettier --write {src,tests}/**/*.ts",
    "prettier:check": "prettier --list-different {src,tests}/**/*.ts"
  },
  "files": [
    "build",
    "protoc-gen-ts_proto"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@grpc/grpc-js": "^1.2.12",
    "@grpc/proto-loader": "^0.5.6",
    "@improbable-eng/grpc-web": "^0.14.0",
    "@improbable-eng/grpc-web-node-http-transport": "^0.14.0",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/microservices": "^7.6.15",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.1.0",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@wix/well-known-protos": "^1.0.441",
    "grpc": "^1.24.6",
    "jest": "^26.6.3",
    "np": "^7.5.0",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7",
    "semantic-release": "^17.4.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "uglify-js": "^3.13.3"
  },
  "dependencies": {
    "protobufjs": "^6.8.8",
    "ts-poet": "^4.5.0",
    "ts-proto-descriptors": "^1.2.1"
  }
}
